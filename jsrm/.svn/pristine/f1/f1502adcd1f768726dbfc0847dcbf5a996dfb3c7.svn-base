<!doctype html>
<html lang="en">
<head>
    <!--meta-->
    <meta charset="UTF-8">
    <!--title-->
    <title>统计分析-工作量统计-个人用户</title>
    <!--link-->
    <link rel="stylesheet" href="../../css/public.css">
    <link rel="stylesheet" href="../../css/edit.css">
    <link rel="stylesheet" href="../../css/jquery.datetimepicker.css">
    <link rel="stylesheet" href="../../css/jquery.selectbox.css">
    <link rel="stylesheet" href="../../css/newStatistics.css">
    <!--script-->
    <script src="../../js/lib/jquery-1.12.1.js"></script>
    <script src="../../js/public.js"></script>
    <script src="../../js/app/reset.js"></script>
    <script src="../../js/lib/jquery.selectbox.js"></script>
    <script src="../../js/lib/jquery.datetimepicker.js"></script>
    <script src="../../js/lib/urlParam.js"></script>
    <!--引入echarts-->
    <script src="../../js/lib/echarts.js"></script>

    <script src="../../js/myApp/baseURL.js"></script>
    <!--<script src="../../js/myApp/newStatistics_classification/phasesubject.js"></script>-->
    <script src="../../js/myApp/newStatistics_classification/getTime.js"></script>
    <!--<script src="../../js/myApp/newStatistics_classification/resource_statistics.js"></script>-->
    <script src="../../js/myApp/newStatistics_classification/statistics_classification_people.js"></script>

</head>
<body>
<div class="main con_fl">
    <!--nav-->
    <!--<div class="topic clearfix">-->
        <!--<ul class="topic_nav">-->
            <!--<li ><a href="statistics.html">资源统计</a></li>-->
            <!--<li class="selected"><a href="statistics_classification.html">工作量统计</a></li>-->
        <!--</ul>-->
        <!--<a href="#" class="btn-normal fr">全部导出</a>-->
    <!--</div>-->
    <!--学段和学科选择-->
    <div class="clearfix phasesubject">
        <div class="phase fl">学段选择</div>
        <div class="fl selectbox selebar selectbox_width126" id="phasesubject1">
            <input name="" value="" type="hidden">
            <div class="selectbox_arraw"><span class="selectbox_down"></span><span class="selectbox_up"></span></div>
            <div class="selectbox_value_box"><div class="selectbox_value">学段</div></div>
            <div class="selectbox_list selectbox_width126">
                <div class="selectbox_scroll_body">
                    <div class="selectbox_scroll"><div></div></div>
                </div>
                <ul class="selectbox_body">
                    <!--<li data-value="0">小学</li>-->
                    <!--<li data-value="0" selected>初中</li>-->
                    <!--<li data-value="0">高中</li>-->
                </ul>
            </div>
        </div>
        <div class="subject fl">学科选择</div>
        <div class="fl selectbox selebar selectbox_width126" id="phasesubject2">
            <input name="" value="" type="hidden">
            <div class="selectbox_arraw"><span class="selectbox_down"></span><span class="selectbox_up"></span></div>
            <div class="selectbox_value_box"><div class="selectbox_value">学科</div></div>
            <div class="selectbox_list selectbox_width126">
                <div class="selectbox_scroll_body">
                    <div class="selectbox_scroll"><div></div></div>
                </div>
                <ul class="selectbox_body">
                    <!--<li data-value="0">语文</li>-->
                    <!--<li data-value="0" selected>数学</li>-->
                    <!--<li data-value="0">化学</li>-->
                    <!--<li data-value="0">英语</li>-->
                    <!--<li data-value="0">体育</li>-->
                </ul>
            </div>
        </div>
        <a href="#" class="btn-normal fr">全部导出</a>
    </div>
    <!--时间-->
    <div class="wrap_date clearfix">
        <p class="timeSelect fl">时间筛选</p>
        <ul class="timeMondth fl">
            <!--<li class="selected" data-date="one">近一个月</li>-->
            <li data-date="one">近一个月</li>
            <li data-date="three">近三个月</li>
            <li data-date="six">近半年</li>
        </ul>
        <div class="clearfix fl">
            <span>自选时间</span>
            <input type="text" id="dates_start1" name="dates_start1" value="">
            <span class="mr10">至</span>
            <input type="text"  id="dates_end1" name="dates_end1" value="">
        </div>
        <span class="btn-normal btn-statistics fr" type="button">统计</span>
    </div>
    <div class="peopleUploadAll clearfix">
        <div class="fl uploadAll">
            <span>个人上传总量：</span>
            <span class="uploadNumber"></span>
        </div>
        <div class="fr">
            <div class="type type_form btn_click">类型</div>
            <div class="form type_form">格式</div>
        </div>
    </div>
    <!--柱状图-->
    <div id="barGraph">
        <div id="barGraphOne" class="bar" style="height:500px;width:auto"></div>
        <div id="barGraphTwo" class="bar" style="height:500px;width:auto"></div>
    </div>
    <!--个人展示-->
    <div class="footer">
        <div class="footer_left f_common">
            <div class="left_bar1">
                <!--<div class="word word_b">发稿统计</div>-->
                <div class="word">
                    <span>个人发稿总量:</span>
                    <div class="word_total topicNum"></div>
                </div>
                <div class="btn1">导出</div>
            </div>
            <div class="left_bar2">
                <div class="green">
                    <div class="green_num topicWord"></div>
                    <div class="green_word">发稿字数(字)</div>
                </div>
                <div class="green green_two">
                    <div class="green_num topicTime"></div>
                    <div class="green_word">音视频时长（秒）</div>
                </div>
                <div class="green green_three">
                    <div class="green_num topicSize"></div>
                    <div class="green_word">资源大小(MB)</div>
                </div>
            </div>
        </div>
        <div class="footer_right f_common">
            <div class="left_bar1">
                <!--<div class="word word_b">发稿统计</div>-->
                <div class="word">
                    <span>个人审核总量:</span>
                    <div class="word_total vettedNumber"></div>
                </div>
                <div class="btn1">导出</div>
            </div>
            <div class="right_bar2">
                <div class="right_top clearfix">
                    <div class="word">
                        <span>上传审核:</span>
                        <div class="word_total uploadvetted"></div>
                    </div>
                    <div class="word word_vetted">
                        <span>发稿审核:</span>
                        <div class="word_total topicvetted"></div>
                    </div>
                </div>
                <div class="right_bottom">
                    <div class="green green_four">
                        <div class="green_num topicrechecked"></div>
                        <div class="green_word">发稿复审</div>
                        <!--<div class="sub_word sub_word_one">-->
                            <!--<p>发稿字数(字)：3333</p>-->
                            <!--<p>音视频时长(秒)：23432</p>-->
                            <!--<p>资源大小(MB)：243423</p>-->
                        <!--</div>-->
                    </div>
                    <div class="green green_five">
                        <div class="green_num topicfinal"></div>
                        <div class="green_word">发稿终审</div>
                        <!--<div class="sub_word sub_word_two">-->
                            <!--<p>发稿字数(字)：3333</p>-->
                            <!--<p>音视频时长(秒)：23432</p>-->
                            <!--<p>资源大小(MB)：243423</p>-->
                        <!--</div>-->
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

</body>
</html>