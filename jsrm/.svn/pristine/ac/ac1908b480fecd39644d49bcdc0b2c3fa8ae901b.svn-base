    <script>
        var itemid = $.getUrlParam("itemId");
        $(function () {
            initBq();
            var result = $.getOperateByType({
                "itemId": itemid,
                "type": ['bar'],
                "path": '/'
            });
            if (result && result.bar) {
                for (var i = 0; i < result.bar.length; i++) {
                    $("#ien").append("<a href=\"javascript:void(0)\" onclick='" + result.bar[i].operatemethod + "()' class=\"upRht\">" + result.bar[i].operatetitle + "</a>");
                }
            }

        });
        function initBq(){
            var html ='<a href="resource.html?itemId='+itemid+'">全部</a>'+
                    '<a href="resource.html?state=1&itemId='+itemid+'">待提交</a>'+
                    '<a href="resource.html?state=2&itemId='+itemid+'">待审核</a>'+
                    '<a href="resource.html?state=3&itemId='+itemid+'" >通过</a>'+
                    '<a href="resource.html?state=4&itemId='+itemid+'">不通过</a>'+
                    '<a href="recycle.html&itemId='+itemid+'" class="selected">回收站</a>';
            $("#ien").append(html);
        }
        function upload(obj){
            window.location.href="../resource/up_orig.html?itemId="+itemid;
        }
    </script>
