.sub_word{
	position: absolute;
	width: 105%;
	padding-left: 10px;
	height:90px;
	line-height:90px;
	background-color: #eaecf2;
	border-radius: 8px;
}
.sub_word_one{
	top:0px;
	left:112%;
	z-index: 10;
}
.sub_word_two{
	top:0px;
	right:112%;
	z-index: 10;
}
.sub_word_one::after{
	content:"";
	position: absolute;
	display: block;
	border-top:10px solid transparent;
	border-bottom:10px solid transparent;
	border-left:6px solid transparent;
	border-right:6px solid #eaecf2;
	top:32px;
	left:-12px;
}
.sub_word_two::after{
	content:"";
	position: absolute;
	display: block;
	border-top:10px solid transparent;
	border-bottom:10px solid transparent;
	border-right:6px solid transparent;
	border-left:6px solid #eaecf2;
	top:32px;
	right:-12px;
}
.sub_word>p {
	height: 23px;
	line-height: 23px;
	margin-top: 2px;
}
.sub_word p:nth-child(1){
	margin-top: 8px;
}

