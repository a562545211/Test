<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>发稿审核</title>
    <script src="../../js/lib/jquery-1.12.1.js"></script>
    <script src="../../js/public.js"></script>
    <link rel="stylesheet" href="../../css/jquery.datetimepicker.css">
    <link rel="stylesheet" href="../../css/jquery.selectbox.css">
    <script src="../../js/lib/jquery.selectbox.js"></script>
    <script src="../../js/app/reset.js"></script>
    <script src="../../js/lib/jquery.datetimepicker.js"></script>
    <link rel="stylesheet" href="../../css/public.css">
    <link rel="stylesheet" href="../../css/edit.css">
    <link rel="stylesheet" href="../../css/alertbox.css">
    <script src="../../js/app/resource_mo.js"></script>

    <script src="../../js/myApp/baseURL.js"></script>

    <script src="../../js/myApp/vetting/selectTransto.js"></script>
    <!-- 分页 -->
    <script src="../../js/lib/pagination.js"></script>
    <script src="../../js/myApp/vetting/topicVetted.js"></script>

</head>
<body>
    <div id="show" class="show" style="padding: 0px;">
        <!--<div class="switch mb">-->
            <!--<div class="edit-word">资源审核</div>-->
            <!--<a href="resourceVetting.html">上传审核</a>-->
            <!--<a href="#" class="selected">发稿审核</a>-->
        <!--</div>-->
        <div class="showbar">
            <div class="switch">
                <div class="edit-word">审核状态</div>
                <a href="topicVetting.html">待审核</a>

            </div>
            <div class="switch content-subnav">
                <a href="javascript:void(0)" class="selected">已审核</a>
            </div>
            <dl>
                <dt class="edit-word">资源编号</dt>
                <dd class="inpbar"><input type="text" id="resourceTopicCode"></dd>
            </dl>
            <dl>
                <dt class="edit-word">资源名称</dt>
                <dd class="inpbar"><input type="text" id="resourceName"></dd>
            </dl>
            <dl>
                <dt class="edit-word">发稿者</dt>
                <dd class="inpbar"><input type="text" id="createUserName"></dd>
            </dl>
            <dl>
                <dt class="edit-word">日期区间</dt>
                <dd class="wrap_date"><input type="text" id="taskStartTime" name="taskStartTime">
                    <span class="mr10">至</span>
                    <input type="text"  id="taskEndTime" name="taskEndTime"></dd>
            </dl>
            <div>
                <button class="btn_cz" id="btn_cz">重置</button>
                <button class="btn_bc" id="btn_bc">查询</button>
            </div>
        </div>
        <div class="wrap_table wrap_table_01" id="resource">
            <div id="pic_no" class="default-prompt"><img src="../../images/default-pic.png"><br><span>暂无待办  ！</span></div>
            <table  border="0" width='100%' cellspacing="0" cellpadding="0" class="table_01" id="table_data">
                <thead>
                    <tr class="tr">
                        <th width="5%"><input type="checkbox" class="all-check"></th>
                        <th>资源编号</th>
                        <th>资源名称</th>
                        <th>资源类型</th>
                        <th>上传者</th>
                        <th>上传时间</th>
                        <th>审核结果</th>
                        <th>审核时间</th>
                        <th>审核意见</th>
                    </tr>
                </thead>
            </table>
            <div class="delBrdr">
                <div class="page" id="page">
                    <div class="pageLft">
                        <!--<a class="noprev"></a>-->
                        <a href="#" class="prev"></a>
                        <a href="#">1</a>
                        <a href="#">2</a>
                        <a href="#">3</a>
                        <a href="#">4</a>
                        <a href="#">5</a>
                        <span>...</span>
                        <a href="#" class="next"></a>
                        <!--<a class="nonext"></a>-->
                    </div>
                    <div class="pageRht">共100页，到第 <input type="text" id="toPage">页 <button>确定</button></div>
                </div>
            </div>
        </div>
    </div>

    <div class="alertboxB btn_xz_alertb" >
        <div class="alertbox alertbox2">
            <h3>选择审核人</h3>
            <div class="alert_sousuo">
                <input type="text" id="userName">
                <button class="btn" id="userQuery">搜索</button>
            </div>
            <div class="wrap_table wrap_table_01" style="height: 190px;" id="wrap_table_0010">
                <table  cellpadding="0" cellspacing="0" class="table_01" id="appUserTable">
                    <thead>
                    <tr>
                        <th></th>
                        <th>姓名</th>
                    </tr>
                    </thead>
                    <tbody>
                    </tbody>
                </table>
            </div>
            <div style="text-align: center; margin-top: 10px; margin-left: -80px;">
                <button class="btn btn_qd">确定</button>
                <button class="btn btn_qx">取消</button>
            </div>

        </div>
    </div>
</body>
</html>