/*框架*/
.frame_top {
    width: 100%;
    height: 64px;
    background: #2196F3;
    position: relative;
    line-height: 64px;
    min-width: 1000px
}

.frame_bottom {
    position: relative;
    left: 0;
    width: 100%;
    min-height: 350px
}

.frame_left {
    width: 230px;
    background: white;
    position: absolute;
    left: 0;
    bottom: 0;
    top: 0
}

.frame_top_btn {
    width: 64px;
    height: 64px;
    background: #0B82DE;
    position: absolute;
    top: 0;
    right: 0;
    background: url('../images/close.png') no-repeat;
    cursor: pointer;
}

.frame_top_btn a {
    display: block;
    width: 100%;
    height: 100%;
}

.frame_left li {
    width: 100%;
    height: 49px;
    border-bottom: 1px solid #eaecf2;
    /*text-align: center;*/
    line-height: 49px;
    vertical-align: middle
}

.frame_left li:hover {
    background: #B2DBFB;
    transition: all 0.6s ease 0s;
}

.frame_left a {
    display: block;
    width: 100%;
    height: 40px;
    color: #666;
    margin-left: 48px;
}

.frame_left span {
    vertical-align: middle;
    width: 38px;
    height: 18px;
    background: url("../images/nav.png") no-repeat;
    display: inline-block;
}

.frame_left span.center {
    background-position: 0 0;
}

.frame_left span.resource {
    background-position: 0 -50px;
}

.frame_left span.electron {
    background-position: 0 -100px;
}

.frame_left span.topic {
    background-position: 0 -150px;
}

.frame_left span.check {
    background-position: 0 -200px;
}

.frame_left span.edit {
    background-position: 0 -250px;
}

.frame_left span.analysis {
    background-position: 0 -300px;
}

.frame_left span.system {
    background-position: 0 -349px;
}

