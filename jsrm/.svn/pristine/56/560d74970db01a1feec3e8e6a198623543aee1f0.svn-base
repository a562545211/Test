.topicDiv{
    margin-bottom:16px;
    height: 50px;
}
.topicDiv .topicSpan{
    float: left;
    display: inline-block;
    margin:8px 0 0 10px;

}
.topicDiv .topicP{
    float: left;
    margin:8px 0 0 10px;
}


.alertboxC{
    position:fixed;
    top:0;
    left:0;
    bottom: 0;
    right:0;
    background:rgba(0,0,0,0.35);
    display: none;
    color:#666666;
    z-index: 9999;
}
.alertTopic{
    width:346px;
    height:187px;
    background: white;
    position: relative;
    text-align: center;
    box-shadow: 0.5px 0.5px 2px 0.5px #dadada;
    margin:10% auto 0;
    /*padding:20px 40px;*/
    /*overflow: hidden;*/
}
.alertImg{
    width:26px;
    height:26px;
    position:absolute;
    top:-39px;
    right:-26px;
}
.alertboxC .alertTopic h2{
    padding: 20px 0 0 0;
    /*text-align: center;*/
}
.alertboxC .alertTopic .alertP{
    padding: 20px;
    line-height: 25px;
    text-align: left;
}
.alertboxC .alertTopic .btn-P{
    text-align: center;
    width: 100%;
    height:30px;
}
.alertboxC .alertTopic .btn-P .alert_btn{
    width: 80px;
    height:30px;
    background-color: rgb(22,155,213);
    color:#fff;
    border-radius: 4px;
    margin-left: 136px;
}
.alertboxD{
    position:fixed;
    top:0;
    left:0;
    bottom: 0;
    right:0;
    background:rgba(0,0,0,0.35);
    display: none;
    color:#666666;
    z-index: 1993;
}
.closealertD{width:26px;height:26px;position:absolute;top:-39px;right:-26px;}

.show .cover{
    display: none;
}
#hover{
    width:36px;
}
#minutes,#seconds{
    width:20px;
}
#second{
    text-align: center;
}