    <script>
        var itemid = $.getUrlParam("itemId");
        $(function () {

            var result = $.getOperateByType({
                "itemId":itemid,
                "type":['bar'],
                "path":'/'
            });
            if(result&&result.bar){
                for(var i=0;i<result.bar.length;i++){
                    $("#upload").append("<a href=\"javascript:void(0)\" onclick='"+result.bar[i].operatemethod+"()' class=\"upRht\">"+result.bar[i].operatetitle+"</a>");
                }
            }
            $("#iem").attr("href","recycle.html?itemId="+itemid);
            //日历组件初始化代码
            $("#dates_start1").datetimepicker({
                value: $("#dates_start1").val(),
                lang: 'ch',
                showSecond: false,
                timepicker: false,
                showMillisec: false,
                format: 'Y-m-d'
            });
            $("#dates_end1").datetimepicker({
                value: $("#dates_end1").val(),
                lang: 'ch',
                showSecond: false,
                timepicker: false,
                showMillisec: false,
                format: 'Y-m-d'
            });
            checkAll("resource");
        });

        function upload(obj){
            window.location.href="../resource/up_orig.html?itemId="+itemid;
        }
    </script>
